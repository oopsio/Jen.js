{
  "name": "@jenjs/jenpress",
  "version": "0.1.0",
  "description": "A VitePress competitor - Markdown-first documentation SSG built with Jen.js, Vite, and Preact",
  "license": "GPL-3.0-only",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "jenpress": "./bin/jenpress.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./theme-default": {
      "import": "./dist/theme-default/index.js"
    }
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "node --import tsx/esm bin/jenpress.js dev",
    "build": "node --import tsx/esm bin/jenpress.js build",
    "serve": "node --import tsx/esm bin/jenpress.js serve",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .jenpress-cache"
  },
  "keywords": [
    "documentation",
    "ssg",
    "static-site-generator",
    "vite",
    "preact",
    "markdown",
    "jenjs"
  ],
  "dependencies": {
    "globby": "^16.1.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.0.0",
    "markdown-it-table-of-contents": "^1.1.0",
    "preact": "^10.25.4",
    "preact-render-to-string": "^6.5.13",
    "shiki": "^1.0.0",
    "tsx": "^4.21.0",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.0.0",
    "@types/node": "^22.10.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
