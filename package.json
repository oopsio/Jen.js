{
  "name": "jen.js-framework",
  "description": "A TypeScript-first framework for building static and server-rendered applications with Preact",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "type": "module",
  "scripts": {
    "================== CORE TASKS ==================": "",
    "dev": "cross-env NODE_ENV=development turbo run dev --parallel",
    "dev:watch": "cross-env NODE_ENV=development turbo run dev --parallel --watch",
    "dev:turbo": "turbo run dev --parallel",
    "start": "cross-env NODE_ENV=production node server.ts start",
    "start:prod": "cross-env NODE_ENV=production node server.ts start",
    "build": "cross-env NODE_ENV=production turbo run build",
    "build:strict": "cross-env NODE_ENV=production turbo run build --force",
    "build:prod": "cross-env NODE_ENV=production npm run clean && npm run build",
    "bundle": "cross-env NODE_ENV=production turbo run bundle",
    "bundle:analyze": "cross-env NODE_ENV=production ANALYZE=true npm run bundle",
    "clean": "turbo run clean && rm -rf dist node_modules .turbo",
    "clean:cache": "turbo prune --docker && rm -rf node_modules .turbo",
    "clean:dist": "turbo run clean",
    "clean:all": "npm run clean && npm run clean:cache",
    "dev:unix": "bash scripts/dev.sh",
    "dev:windows": "powershell -ExecutionPolicy Bypass -File scripts/dev.ps1",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* turbo run dev",
    "dev:inspect": "cross-env NODE_ENV=development node --inspect-brk server.ts dev",
    "dev:hmr": "cross-env NODE_ENV=development HMR_ENABLED=true npm run dev",
    "dev:fast": "turbo run dev --filter=!example --filter=!website",
    "dev:server": "cross-env NODE_ENV=development node server.ts dev",
    "dev:build": "cross-env NODE_ENV=development node build.ts",
    "dev:bundle": "cross-env NODE_ENV=development node bundle.js",
    "build:unix": "bash scripts/build.sh",
    "build:windows": "powershell -ExecutionPolicy Bypass -File scripts/build.ps1",
    "build:ssg": "cross-env NODE_ENV=production node build.ts",
    "build:esm": "cross-env NODE_ENV=production NODE_BUILD_ESM=true npm run build",
    "build:cjs": "cross-env NODE_ENV=production NODE_BUILD_CJS=true npm run build",
    "build:types": "cross-env NODE_ENV=production node build.ts --types",
    "build:sourcemap": "cross-env NODE_ENV=production SOURCEMAP=true npm run build",
    "build:minify": "cross-env NODE_ENV=production MINIFY=true npm run build",
    "build:check": "npm run typecheck && npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage.reporter=html",
    "test:debug": "vitest --inspect-brk --inspect --no-coverage",
    "test:unit": "vitest run --include='**/*.test.ts' --exclude='**/*.e2e.ts'",
    "test:unit:watch": "vitest --include='**/*.test.ts' --exclude='**/*.e2e.ts'",
    "test:integration": "vitest run --include='**/*.integration.ts'",
    "test:e2e": "cross-env TEST_ENV=e2e playwright test",
    "test:e2e:ui": "cross-env TEST_ENV=e2e playwright test --ui",
    "test:e2e:debug": "cross-env TEST_ENV=e2e playwright test --debug",
    "test:perf": "vitest run --include='**/*.perf.ts'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:changed": "vitest run --changed",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --fix",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --max-warnings 0",
    "lint:report": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --format json > lint-report.json",
    "lint:css": "stylelint 'src/**/*.{css,scss}'",
    "lint:css:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "lint:md": "alex . && prettier --check '**/*.md'",
    "lint:types": "tsc --noEmit",
    "lint:security": "npm audit --audit-level=moderate",
    "lint:dependencies": "npm ls --all",
    "format": "prettier --write . --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown",
    "format:staged": "lint-staged",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "typecheck:strict": "cross-env STRICT_TYPES=true tsc --noEmit",
    "typecheck:incremental": "tsc --noEmit --incremental",
    "setup": "bash scripts/setup.sh || powershell -File scripts/setup.ps1",
    "setup:unix": "bash scripts/setup.sh",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup.ps1",
    "setup:deps": "pnpm install",
    "setup:env": "node scripts/setup-env.js",
    "setup:dev": "npm run setup:env && npm run setup:deps && npm run typecheck",
    "setup:git-hooks": "husky install",
    "db:migrate": "cross-env NODE_ENV=production node scripts/db-migrate.js",
    "db:migrate:dev": "cross-env NODE_ENV=development node scripts/db-migrate.js",
    "db:seed": "cross-env NODE_ENV=development node scripts/db-seed.js",
    "db:reset": "cross-env NODE_ENV=development npm run db:migrate:dev && npm run db:seed",
    "db:status": "node scripts/db-status.js",
    "db:backup": "node scripts/db-backup.js",
    "perf:analyze": "cross-env ANALYZE=true npm run build",
    "perf:bundle": "webpack-bundle-analyzer dist/stats.json",
    "perf:lighthouse": "lighthouse https://localhost:3000 --view",
    "perf:bench": "vitest run --include='**/*.bench.ts'",
    "perf:profile": "cross-env PROFILE=true npm run build",
    "perf:metrics": "node scripts/metrics.js",
    "deploy": "cross-env NODE_ENV=production npm run build && npm run deploy:push",
    "deploy:push": "node scripts/deploy.js",
    "deploy:staging": "cross-env DEPLOY_ENV=staging npm run deploy",
    "deploy:prod": "cross-env DEPLOY_ENV=production npm run deploy",
    "deploy:rollback": "node scripts/rollback.js",
    "deploy:health": "node scripts/health-check.js",
    "docker:build": "docker build -t jen.js:latest .",
    "docker:build:dev": "docker build -t jen.js:dev -f Dockerfile.dev .",
    "docker:run": "docker run -p 3000:3000 jen.js:latest",
    "docker:push": "docker push jen.js:latest",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docs": "cross-env NODE_ENV=development node scripts/docs-server.js",
    "docs:build": "cross-env NODE_ENV=production node scripts/docs-build.js",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:validate": "node scripts/validate-docs.js",
    "cli:create": "cross-env NODE_ENV=development tsx src/cli/index.ts",
    "cli:create:project": "tsx src/cli/index.ts create",
    "cli:create:component": "tsx src/cli/index.ts create:component",
    "cli:create:page": "tsx src/cli/index.ts create:page",
    "cli:create:middleware": "tsx src/cli/index.ts create:middleware",
    "codegen": "node scripts/codegen.js",
    "codegen:graphql": "graphql-codegen --config codegen.yml",
    "codegen:types": "tsx scripts/generate-types.ts",
    "codegen:routes": "tsx scripts/generate-routes.ts",
    "codegen:api": "tsx scripts/generate-api.ts",
    "validate": "npm run typecheck && npm run lint && npm run test:run",
    "validate:ci": "npm run typecheck && npm run lint:strict && npm run test:run && npm run test:e2e",
    "ci": "npm run validate:ci && npm run build",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "update:deps": "npm outdated && npm update",
    "update:deps:major": "npm outdated && npm update --save",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:check": "node scripts/security-check.js",
    "packages": "turbo run --filter='./packages/*'",
    "packages:build": "turbo run build --filter='./packages/*'",
    "packages:test": "turbo run test --filter='./packages/*'",
    "packages:lint": "turbo run lint --filter='./packages/*'",
    "packages:version": "lerna version --no-push",
    "packages:publish": "lerna publish",
    "packages:changed": "lerna changed",
    "native:build": "node scripts/build-native.js",
    "native:build:debug": "cross-env DEBUG=true node scripts/build-native.js",
    "native:test": "vitest run --include='**/native/**/*.test.ts'",
    "bench": "vitest run --include='**/*.bench.ts'",
    "bench:compare": "node scripts/benchmark-compare.js",
    "bench:regression": "node scripts/benchmark-regression.js",
    "release": "node scripts/release.js",
    "release:major": "cross-env RELEASE_TYPE=major npm run release",
    "release:minor": "cross-env RELEASE_TYPE=minor npm run release",
    "release:patch": "cross-env RELEASE_TYPE=patch npm run release",
    "release:prerelease": "cross-env RELEASE_TYPE=prerelease npm run release",
    "release:changelog": "node scripts/generate-changelog.js",
    "ci:lint": "npm run lint:strict",
    "ci:test": "npm run test:run -- --coverage",
    "ci:build": "npm run build:strict",
    "ci:e2e": "npm run test:e2e",
    "ci:all": "npm run ci:lint && npm run ci:test && npm run ci:build && npm run ci:e2e",
    "prune": "turbo prune --docker",
    "prune:strict": "turbo prune --docker && npm run clean:cache",
    "nuke": "npm run clean:all && npm run setup:deps",
    "monitor": "node scripts/monitor.js",
    "monitor:metrics": "node scripts/monitor-metrics.js",
    "monitor:health": "node scripts/health-check.js",
    "monitor:logs": "node scripts/view-logs.js",
    "migrate": "node scripts/migrate.js",
    "migrate:create": "node scripts/migrate-create.js",
    "migrate:list": "node scripts/migrate-list.js",
    "migrate:rollback": "node scripts/migrate-rollback.js",
    "script:list": "node scripts/list-scripts.js",
    "script:help": "node scripts/help.js"
  },
